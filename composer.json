{
    "name": "tsmsogn/solidus",
    "description": "Solidus for CakePHP",
    "type": "cakephp-plugin",
    "license": "New BSD License",
    "require": {
        "cakephp/cakephp": "^3.4",
        "tsmsogn/discard": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Solidus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solidus\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
